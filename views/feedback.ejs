<% include partials/header %>


<div class="container">
    <div class="card">
        <div class="card-header">
            Put your Feedback here!!!!
        </div>
        <div class="card-body">
            <form action="">
                <div class="form-group">
                    <label for="exampleInputEmail1" class="bmd-label-floating">Email address</label>
                    <input type="email" name="email" class="form-control" id="exampleInputEmail1">
                    <span class="bmd-help">We'll never share your email with anyone else.</span>
                </div>
                <div class="form-group">
                    <label for="exampleTextarea" class="bmd-label-floating">Your Feedback</label>
                    <textarea name="feedback" class="form-control" id="exampleTextarea" rows="3"></textarea>
                </div>
                <button type="submit" style="background-color:#263238" class="btn btn-raised btn-info">Submit feedback</button>
            </form>
        </div>
    </div>
</div>

